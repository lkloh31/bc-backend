    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "react-infinite-scroll-component": "^6.1.0",
    "request-ip": "^3.3.0"
  },
  "engines": {
    "node": ">=22"
  }
