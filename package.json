{
  "name": "bc-backend",
  "type": "module",
  "imports": {
    "#*": "./*.js"
  },
  "scripts": {
    "start": "node --env-file=.env server.js",
    "dev": "node --watch --env-file=.env server.js",
    "db:schema": "psql -d bc -f db/schema.sql",
